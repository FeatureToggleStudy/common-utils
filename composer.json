{
    "name" : "ministryofjustice/common-utils",
    "description" : "Common code that is needed for all projects",
    "keywords": [
        "ministryofjustice",
        "opg"
    ],
    "homepage": "https://github.com/ministryofjustice/common-utils",
    "type": "library",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=5.5",
        "psr/log": "^1.0",
        "symfony/debug": ">=2.6.0, <4.0",
        "zendframework/zendframework": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.8.0"
    },
    "autoload" : {
        "psr-4" : {
            "CommonUtils\\" : "src/CommonUtils/",
            "CommonUtilsTest\\": "tests/CommonUtilsTest/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
